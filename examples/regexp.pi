import regexp

pattern = "w([a-z]+)d"
target = "hello world"
matched = regexp.match(pattern, target)
print(matched)

substr = regexp.find(pattern, target)
print(substr)

pos = regexp.index(pattern, target)
print(pos)

re = regexp.compile(pattern)
matched = re.match(target)
print(matched)

substr = re.find(target)
print(substr)

pos = re.index(target)
print(pos)